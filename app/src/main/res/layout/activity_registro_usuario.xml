            android:layout_width="348dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:ems="15"
            android:inputType="textPersonName" />

        <TextView
            android:id="@+id/textView1"
            android:layout_width="348dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="85dp"
